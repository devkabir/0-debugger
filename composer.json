{
    "name": "devkabir/wp-debugger",
    "description": "A simple tool to help you finding any error on wordpress website.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Dev Kabir",
            "email": "dev.kabir01@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "lock": false
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "filp/whoops": "^2.15",
        "maximebf/debugbar": "1.23.x-dev",
        "symfony/var-dumper": "5.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "DevKabir\\WPDebugger\\": "src/"
        }
    }
}
